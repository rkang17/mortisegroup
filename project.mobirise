          "_name": "footer1",
          "_customHTML": "<section class=\"footer1\" group=\"Footers\" plugins=\"amp-main\" global once=\"footer\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"1\">\n\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"1\">\n\n\t<input type=\"checkbox\" title=\"Show Items\" name=\"showItems\" checked>\n\t<input type=\"range\" inline title=\"Items Count\" name=\"itemsCount\" min=\"1\" max=\"8\" step=\"1\" value=\"7\" condition=\"showItems\">\n\t<input type=\"checkbox\" title=\"Show Copyright\" name=\"showCopyright\" checked>\n\t<!-- Background -->\n\t<input type=\"color\" title=\"Background Color\" name=\"bg\" value=\"#18184c\">\n\t<input type=\"checkbox\" title=\"Show BG Figures\" name=\"bgFigures\">\n\t<!-- Select Figure -->\n\t<select title=\"Select Figure:\" name=\"figure\" condition=\"bgFigures\">\n\t\t<option value=\"1\">Circle</option>\n\t\t<option value=\"2\" selected>Rectangle</option>\n\t\t<option value=\"3\">Diagonal</option>\n\t</select>\n\t<!-- Figure Circle -->\n\t<input type=\"range\" inline title=\"Circle X\" name=\"circlePosX\" min=\"-100\" max=\"100\" step=\"5\" value=\"40\" condition=\"figure==1 && bgFigures\">\n\t<input type=\"range\" inline title=\"Circle Y\" name=\"circlePosY\" min=\"-100\" max=\"100\" step=\"5\" value=\"-40\" condition=\"figure==1 && bgFigures\">\n\t<input type=\"range\" inline title=\"Circle Radius\" name=\"circleRadius\" min=\"100\" max=\"1600\" step=\"50\" value=\"1200\" condition=\"figure==1 && bgFigures\">\n\t<input type=\"color\" title=\"Circle Color\" name=\"circleColor\" value=\"#9dc7d3\" condition=\"figure==1 && bgFigures\">\n\t<input type=\"checkbox\" title=\"Overflow Hidden\" name=\"overflow\" condition=\"figure==1 && bgFigures\" checked>\n\t<!-- Figure Rectangle -->\n\t<input type=\"checkbox\" title=\"Width/Height\" name=\"directRect\" condition=\"figure==2 && bgFigures\" checked>\n\t<input type=\"checkbox\" title=\"Invert\" name=\"invertRect\" condition=\"figure==2 && bgFigures\" checked>\n\t<input type=\"range\" inline title=\"Size\" name=\"sizeRect\" min=\"0\" max=\"100\" step=\"5\" value=\"55\" condition=\"figure==2 && bgFigures\">\n\t<input type=\"color\" title=\"Rectangle Color\" name=\"rectColor\" value=\"#007873\" condition=\"figure==2 && bgFigures\">\n\t<!-- Figure Diagonal -->\n\t<input type=\"range\" inline title=\"Rotate Angle\" name=\"rotateAngle\" min=\"0\" max=\"360\" step=\"5\" value=\"45\" condition=\"figure==3 && bgFigures\">\n\t<input type=\"range\" inline title=\"Size\" name=\"sizeDiagonal\" min=\"0\" max=\"100\" step=\"5\" value=\"50\" condition=\"figure==3 && bgFigures\">\n\t<input type=\"color\" title=\"Diagonal Color\" name=\"diagonalColor\" value=\"#9dc7d3\" condition=\"figure==3 && bgFigures\">\n\t<!-- End block parameters -->\n\t</mbr-parameters>\n\t<div class=\"figure\" mbr-if=\"bgFigures\"></div>\n\t<div class=\"container\">\n\t\t<div mbr-list mbr-list-grow=\"itemsCount\" class=\"mbr-row\" mbr-if=\"showItems\">\n\t\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"home.html\" class=\"text-white\">\n\t\t\t\t\tHome\n\t\t\t</a></p>\n\t\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"about.html\" class=\"text-white\">\n\t\t\t\t\tAbout Us\n\t\t\t</a></p>\n\t\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"projects.html\" class=\"text-white\">\n\t\t\t\t\tProjects</a></p>\n\t\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"home-care.html\" class=\"text-white\">\n\t\t\t\t\tHome Care</a></p>\n\t\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"contact.html\" class=\"text-white\">\n\t\t\t\t\tContact Us</a></p>\n\t\t<p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><a href=\"privacy.html\">Privacy Policy</a></p><p class=\"mbr-text fLink mbr-white mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\"fLink\"><br></p></div>\n\t\t<div class=\"container\" mbr-if=\"showCopyright\">\n\t\t\t<div class=\"mbr-col-sm-12 align-center\">\n\t\t\t\t<p class=\"mbr-text mbr-white mbr-fonts-style copyright\" mbr-theme-style=\"display-6\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">\n\t\t\t\t\t© Copyright 2019 Mortise Group of Companies</p>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</section>",
